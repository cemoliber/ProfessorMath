        android:layout_marginTop="2dp" />

    <FrameLayout
        android:id="@+id/frame4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/frame3"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="20dp">

        <ProgressBar
            android:id="@+id/progressBar4"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:max="100"
            android:progress="100"
            android:layout_gravity="end"
            android:indeterminate="false"
            android:progressDrawable="@drawable/progress1"/>

        <TextView
            android:id="@+id/progressText4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0%"
            android:textColor="#FFFFFF"
            android:layout_gravity="center" />
    </FrameLayout>

    <View
        android:id="@+id/line4"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="#63000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/frame3"
        android:layout_marginTop="10dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="BÖLME"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@+id/line4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />

    <TextView
        android:id="@+id/textView4_1"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:text="Çözülen Soru: 0"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/frame4"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/textView4_2"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:text="Doğru Sayısı: 0"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textView4_1"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="2dp" />

    <TextView
        android:id="@+id/textView4_3"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:text="Yanlış Sayısı: 0"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textView4_2"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="2dp" />

</androidx.constraintlayout.widget.ConstraintLayout>
